{
    "output_path": "~/cheri/cheri-benchplot/demo/demo-netperf-multi-instance",
    "sdk_path": "~/cheri/cherisdk",
    "ssh_key": "~/cheri/cheri-benchplot/extra_files/freebsd_test_id_rsa",
    "cheribuild_path": "~/cheri/cheribuild/cheribuild.py",
    "perfetto_path": "~/cheri/cheri-perfetto/build",
    "instances": [
        {
            "baseline": true,
            "terminate_on_reset": true,
            "kernel": "CHERI-QEMU-NODEBUG",
            "platform": "qemu",
            "cheri_target": "riscv64-purecap",
            "kernelabi": "hybrid",
            "platform_options": {
                "qemu_trace": true,
                "qemu_trace_categories": ["ctrl", "stats"]
            }
        },
        {
            "baseline": false,
            "terminate_on_reset": true,
            "kernel": "CHERI-PURECAP-QEMU-NODEBUG",
            "platform": "qemu",
            "cheri_target": "riscv64-purecap",
            "kernelabi": "purecap",
            "platform_options": {
                "qemu_trace": true,
                "qemu_trace_categories": ["ctrl", "stats"]
            }
        }
    ],
    "benchmarks": [
        {
            "name": "udp_rr_50_1b",
            "desc": "UDP RR 5K transactions 1-byte",
            "type": "netperf",
            "output_file": "{netperf_output}",
            "extra_files": ["{netperf_pmc}"],
            "benchmark_options": {
                "qemu_log_output": "{netperf_qemu_stats}",
                "netperf_options": [
                    "-P", "1",
                    "-i", "3,3",
                    "-g", "qemu-thread",
                    "-G", "{netperf_pmc}",
                    "-l", "-50",
                    "-t", "UDP_RR", "--",
                    "-o", "SOCKET_TYPE,PROTOCOL,DIRECTION,ELAPSED_TIME,THROUGHPUT,THROUGHPUT_UNITS,REQUEST_SIZE,RESPONSE_SIZE,CONFIDENCE_ITERATION,UUID,NETPERF_ABI,KERNEL_ABI,COMMAND_LINE",
                    "-r", "1,1",
                    "-u", "{uuid}"],
                "netserver_options": ["-D"]
            },
            "datasets": {
                "netperf-output": {
                    "name": "netperf-output-{uuid}.csv",
                    "parser": "netperf-data"
                },
                "netperf-pmc": {
                    "name": "netperf-pmc-{uuid}.csv",
                    "parser": "pmc"
                },
                "netperf-qemu-stats": {
                    "name": "netperf-qemu-stats-{uuid}.pb",
                    "parser": "qemu-stats-bb"
                },
                "netperf-qemu-stats-call": {
                    "name": "netperf-qemu-stats-{uuid}.pb",
                    "parser": "qemu-stats-call"
                },
                "netperf-vmstat-malloc": {
                    "name": "vmstat-{uuid}.json",
                    "parser": "vmstat-malloc"
                },
                "netperf-vmstat-uma": {
                    "name": "vmstat-{uuid}.json",
                    "parser": "vmstat-uma"
                }
            }
        }
    ]
}
